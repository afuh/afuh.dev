{
  "semi": false,
  "arrowParens": "always",
  "singleQuote": true,
  "bracketSpacing": true,
  "trailingComma": "all",
  "tabWidth": 2,
  "printWidth": 120,
  "plugins": ["prettier-plugin-astro"],
  "overrides": [
    {
      "files": ["**/*.astro"],
      "options": {
        "parser": "astro"
      }
    }
  ]
}
